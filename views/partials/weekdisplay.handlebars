<div id="weeklist" class="flex-row">
  {{#each fullWeek}}
    <div class="test weekday">
      <div>
        <h3>{{weekday.day_name}}</h3>
        <h4>{{format_date date}}</h4>
      </div>
      <div class="priority text">
        <h5>Priority:</h5>
        <ul>
          {{#each priorities}}
            <li>{{content}}</li>
          {{/each}}
        </ul>
      </div>
      <div class="todos text">
        <h5>To-Do:</h5>
        <ul>
          {{#each tasks}}
            <li>{{content}}</li>
          {{/each}}
        </ul>
      </div>
    </div>
  {{/each}}

  {{! <div class="test weekday">
    <h3>Mon</h3>
    <h4>Date</h4>
    <div class="priority">Priority:</div>
    <div class="todos">To-Do's</div></div>
  <div class="test weekday">
    <h3>Tue</h3>
    <h4>Date</h4>
    <div class="priority">Priority:</div>
    <div class="todos">To-Do's</div></div>
  <div class="test weekday">
    <h3>Wed</h3>
    <h4>Date</h4>
    <div class="priority">Priority:</div>
    <div class="todos">To-Do's</div></div>
  <div class="test weekday">
    <h3>Thu</h3>
    <h4>Date</h4>
    <div class="priority">Priority:</div>
    <div class="todos">To-Do's</div></div>
  <div class="test weekday">
    <h3>Fri</h3>
    <h4>Date</h4>
    <div class="priority">Priority:</div>
    <div class="todos">To-Do's</div></div>
  <div class="test weekday">
    <h3>Sat</h3>
    <h4>Date</h4>
    <div class="priority">Priority:</div>
    <div class="todos">To-Do's</div></div>
}}

</div>